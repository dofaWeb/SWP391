
@{
    ViewData["Title"] = "GetProductByBrand";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h2 class="text-center">@ViewBag.brand Product</h2>
<div class="row">
    @await Component.InvokeAsync("ProductCustomer", new
        {
            productType = "GetProductByBrand",
            str = ViewBag.brand
        })
</div>

