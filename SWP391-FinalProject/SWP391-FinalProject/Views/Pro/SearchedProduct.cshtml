@model IEnumerable<SWP391_FinalProject.Models.ProductModel>
@{
    ViewData["Title"] = "SearchedProduct";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2 class="text-center">Searched Product</h2>
<div class="row">
    @await Component.InvokeAsync("ProductCustomer", new
        {
            productType = "GetProductsByKeyword",
            str = ViewBag.keyword
        })
</div>